{
  "name": "@tabianco/vue-inherit-component",
  "version": "2.2.3",
  "description": "Overrides default behavior of pre-defined Vue2 components",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabianco/vue-inherit-component.git"
  },
  "author": "Tabian Co.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tabianco/vue-inherit-component/issues"
  },
  "homepage": "https://github.com/tabianco/vue-inherit-component/tree/main/packages/v2#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@vue/composition-api": "^1.1.3",
    "@vue/test-utils": "^1.2.2",
    "jest": "^27.1.0",
    "jest-serializer-vue": "^2.0.2",
    "ts-jest": "^27.0.5",
    "vue": "^2.6.14",
    "vue-jest": "^3.0.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/))spec\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  }
}
